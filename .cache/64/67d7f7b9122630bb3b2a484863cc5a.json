od_good.js":"'use strict';\n\nvar $ = require('jquery');\nvar Util = require('./ajax/util');\n\nvar page = 1;\nvar $foods = $('.js-foods');\nvar $more = $('.js-more');\n\nfunction getMoreFood() {\n  var currentPage = ++page;\n\n  var request = Util.myAjax({\n    url: '/home/food/more',\n    data: {\n      page: currentPage\n    },\n    method: 'post'\n  });\n\n  request.done(function(ret) {\n    if (ret.errno === 0) {\n      var data = ret.data || [];\n      var len = data.length;\n      if (len === 0) {\n        $more.html('亲，已经到底了～')\n          .off('click.more')\n          .delay(1500)\n          .slideUp();\n      } else if (len > 0) {\n        var tmpl = [];\n        data.forEach(function(item) {\n          var cur = Util.getTmpl({\n            url: item.foodurl,\n            name: item.foodname\n          });\n\n          tmpl.push(cur);\n        });\n        var moreFood = tmpl.join('');\n\n        var $moreFood = $(moreFood).hide()\n          .addClass('more-food')\n          .attr('index', 'more' + currentPage)\n          .css('transform', 'rotate(0deg)');\n        $foods.append($moreFood);\n        $('.more-food').show()\n          .css('transform', 'rotate(360deg)');\n      }\n    }\n  });\n}\n\n$more.on('click.more', function(e) {\n  e.preventDefault();\n  getMoreFood();\n});\n"},"lineCount":null}},"error":null,"hash":"339c0ce54bd12e61077470dd0a93c226","cacheData":{"env":{}}}